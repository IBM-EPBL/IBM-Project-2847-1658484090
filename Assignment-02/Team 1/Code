import random
import time
while True:
    '''a = Temperature
    b = Humidity'''
    a = random.randint(25, 203)
    b = random.randint(0, 100)
    print("Temp is: "+str(a)+"ᵒF") 
    print("Temp in celsius: "+str(((a-32)*5)//9)+"?")
    print("The Humidity is: "+str(b) + "%")
    if (a >= 100):
        print("High Temp Alert: "+str(a)+"ᵒF")
    time.sleep(10)
