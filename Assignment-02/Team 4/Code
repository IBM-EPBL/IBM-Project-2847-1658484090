import random
import time
while True:
    t = random.randint(25, 203)
    h = random.randint(0, 100)
    print("Temp is: "+str(t)+"ᵒF") 
    print("Temp in celsius: "+str(((t-32)*5)//9)+"?")
    print("The Humidity is: "+str(h) + "%")
    if (t >= 100):
        print("High Temp Alert: "+str(t)+"ᵒF")
    time.sleep(10)
